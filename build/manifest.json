{
 "manifest_version": 2,
 "name": "My Schedule",
 "version": "0.0.1",

 "description": "Chrome Extension for tracking a student schedule on a rotating x-day cycle",

 "browser_action": {

    "default_icon": {
        "16": "icons/icon16.png",
        "24": "icons/icon24.png",
        "32": "icons/icon32.png"
    },

    "default_title": "My Schedule",
    "default_popup": "index.html"
 } 

}